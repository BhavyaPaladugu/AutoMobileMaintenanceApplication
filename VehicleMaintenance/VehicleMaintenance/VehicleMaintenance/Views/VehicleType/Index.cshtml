@model IEnumerable<VehicleMaintenance.DAL.Models.VehicleType>

@{
    ViewBag.Title = "Vehicle Types - Display";
}

<p>
    @Html.ActionLink("Add New Vehicle Type", "Create")
</p>

@if (Model.Count() != 0)
{
    <h3>Available Vehicle Types</h3>
    <table class="table">
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.VehicleTypeName)
                </td>
                <td>
                    @Html.ActionLink("Edit", "Edit", new { id = item.VehicleTypeId }) |
                    @Html.ActionLink("Delete", "Delete", new { id = item.VehicleTypeId })
                </td>
            </tr>
        }
    </table>
}




