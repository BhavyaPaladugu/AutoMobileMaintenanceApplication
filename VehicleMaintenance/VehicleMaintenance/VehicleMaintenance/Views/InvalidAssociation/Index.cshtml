@model IEnumerable<VehicleMaintenance.DAL.Models.InvalidAssociation>

@{
    ViewBag.Title = "Invalid Associations - Display";
}

<p>
    @Html.ActionLink("Add New Invalid Association", "Create")
</p>

@if (Model.Count() != 0)
{
    <h3>Invalid Associations</h3>
    <table class="table">
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.VehicleType.VehicleTypeName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Task.TaskName)
                </td>
                <td>
                    @Html.ActionLink("Edit", "Edit", new { id = item.InvalidAssociationId }) |
                    @Html.ActionLink("Delete", "Delete", new { id = item.InvalidAssociationId })
                </td>
            </tr>
        }
    </table>
}




